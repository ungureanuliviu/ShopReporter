<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout 
	xmlns:android="http://schemas.android.com/apk/res/android" 
	android:layout_width="fill_parent"
	android:layout_height="wrap_content"
	android:background="@drawable/bg_autocomplete_item"	
	android:focusable="false"
	android:focusableInTouchMode="false">
	<TextView
		android:id="@+id/ac_name"
	    android:singleLine="true"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_alignParentLeft="true"
	    android:layout_marginLeft="5dp"	    
	    android:textColor="#ffffff"
	    android:textSize="15dp"
		android:layout_marginTop="2dp"
	    android:text = "test name"	    
	    android:ellipsize="end"
	    android:layout_toLeftOf="@+id/ac_but_fill_all"/>	
	<Button 
		android:id="@id/ac_but_fill_all"
		android:layout_width="70dp"
		android:layout_height="30dp"
		android:text = "Fill all fields"
		android:textColor="#ffffff"
		android:textSize="11dp"
		android:layout_alignParentRight="true"
		android:layout_centerVertical="true"
		android:layout_marginRight="5dp"		
		android:focusableInTouchMode="false"
		android:focusable="false"
		android:background="@drawable/button_background"/>	    
	<TextView
		android:id="@+id/ac_date"
	    android:singleLine="true"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_alignParentLeft="true"
	    android:layout_below="@id/ac_name"
	    android:layout_marginLeft="5dp"	    
	    android:textColor="#ffffff"
	    android:textSize="10dp"
	    android:text = "Last added: 10-11-2010"/>	    
	<TextView
		android:id="@+id/ac_price"
	    android:singleLine="true"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_alignParentLeft="true"
	    android:layout_below="@id/ac_date"
	    android:layout_marginLeft="5dp"	    
	    android:textColor="#ffffff"
	    android:textSize="11dp"
	    android:text = "Last price: $11.01"/>	 	    
	    
</RelativeLayout>
